			<texPath>2B/Anneli_Gakuen_Miniskirt</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<apparel>
			<wornGraphicPath>2B/Anneli_Gakuen_Miniskirt</wornGraphicPath>
			<tags>
				<li>Anneli_Gakuen_Common</li>
			</tags>
			<useWornGraphicMask>true</useWornGraphicMask><!--Mask Applied-->
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(75,65,70,255)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>

